{
  "name": "cenksen/laravel-kolaybi",
  "description": "KolayBi API for Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Cenksen\\Kolaybi\\": "src/"
    }
  },
  "authors": [
    {
      "name": "cenksen",
      "email": "dev@cenksen.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.00"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cenksen\\Kolaybi\\KolayBiServiceProvider"
      ],
      "aliases": {
        "KolayBi": "Cenksen\\Kolaybi\\Facades\\KolayBi"
      }
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.18"
  }
}
